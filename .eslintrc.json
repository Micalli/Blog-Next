{
  "env": {
      "browser": true,
      "es2021": true,
      "amd": true,
      "node": true
  },
  "extends": [
      "plugin:react/recommended",
      "eslint:recommended",
      "prettier",
      "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 13,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "@typescript-eslint"
  ],
  "rules": {
    "no-unused-expressions":"off",
    "react/react-in-jsx-scope":"off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}

